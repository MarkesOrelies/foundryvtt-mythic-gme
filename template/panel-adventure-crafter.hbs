<script>

    $.fn.moveUp = function () {
        before = $(this).prev();
        $(this).insertBefore(before)
    }

    $.fn.moveDown = function () {
        after = $(this).next();
        $(this).insertAfter(after)
    }

    $('.tac-dialog__up').click(function () {
        $(this).parent().moveUp()
    })

    $('.tac-dialog__down').click(function () {
        $(this).parent().moveDown()
    })

</script>